{
  "public": true,
  "trailingSlash": true,
  "cleanUrls": true,
  "redirects": [
    {
      "source": "/(faq|who|now|about)(/*)",
      "destination": "/#about"
    },
    {
      "source": "/(contact)(/*)",
      "destination": "/#contact"
    },
    {
      "source": "/(flickr|photography|photos|photog)(/*)",
      "destination": "https://www.flickr.com/people/imtom/"
    },
    {
      "source": "/(flickr|photography|photos|photog)/(map|photosets|photos)(/*)",
      "destination": "https://github-pages.thomasr.me/flickr-toys/"
    },
    {
      "source": "/flickr/map/stations(/*)",
      "destination": "https://www.google.com/maps/d/edit?mid=1I6N53Rx0baiZX8nbl4yVGC9R3YXLGXM"
    },
    {
      "source": "/humans:slug*",
      "destination": "https://github.com/itsmeimtom/thomasr.me/graphs/contributors"
    },
    {
      "source": "/(cv|resume)(/*)",
      "destination": "https://cv.thomasr.me"
    },
    {
      "source": "/projects/twitterbot(/*)",
      "destination": "/twitterbot"
    },
    {
      "source": "/projects/aog-privacy(/*)",
      "destination": "/aog-privacy"
    },
    {
      "source": "/(faq/tech|faq/devices|tech|devices)(/*)",
      "destination": "/uses"
    },
    {
      "source": "/homelab(/*)",
      "destination": "/lab"
    },
    {
      "source": "/(solihull|solcol)(/*)",
      "destination": "https://solcol.thomasr.me"
    },
    {
      "source": "/(notion-at)(/*)",
      "destination": "https://github.com/tomatsolihull/notion-assignment-tracker/blob/master/README.md"
    },
    {
      "source": "/(projects|project|unsub|gogo|33rpm|speedtest-map|zrpc|boris|supercutter|mettranslinksystem)(/*)",
      "destination": "/#projects"
    },
    {
      "source": "/(project|thing)(s?)/(\\w*)(/*)",
      "destination": "/#projects"
	}
  ],
  "rewrites": [],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Powered-By",
          "value": "hugs"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; font-src 'self'; connect-src 'self' https://github-pages.thomasr.me; img-src 'self' data:; frame-src 'self';"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Powered-By",
          "value": "hugs"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; font-src 'self'; connect-src 'self' https://github-pages.thomasr.me; img-src 'self' data:; frame-src 'self';"
        }
      ]
    }
  ]
}
